{
  "name": "pack-opening-boilerplate",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "echo \"Attention: front-end client and cloudflare function development use different tools. \n\nPlease be more specific: \n\npnpm run dev:vite \npnpm run dev:cf\"",
    "dev:vite": "vite",
    "dev:cf": "concurrently \"pnpm run dev:cf:functions\" \"pnpm run dev:cf:build\"",
    "dev:cf:functions": "wrangler pages dev",
    "dev:cf:build": "nodemon  --watch 'src/**/*' -e ts,tsx --exec \"pnpm run build\"",
    "create-packs-csv": "tsx scripts/create-packs-csv.ts",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "postinstall": "(cp -n .env.example .env) || echo already exists"
  },
  "dependencies": {
    "0xsequence": "2.3.7",
    "@0xsequence/api": "2.3.7",
    "@0xsequence/auth": "^2.3.8",
    "@0xsequence/checkout": "^5.0.9",
    "@0xsequence/connect": "^5.0.9",
    "@0xsequence/design-system": "^2.1.6",
    "@0xsequence/indexer": "^2.3.7",
    "@0xsequence/metadata": "2.3.7",
    "@0xsequence/network": "^2.3.7",
    "@react-three/drei": "^10.0.6",
    "@react-three/fiber": "^9.1.2",
    "@react-three/postprocessing": "^3.0.4",
    "@tanstack/react-query": "^5.71.5",
    "boilerplate-design-system": "github:0xsequence-demos/boilerplate-design-system#42564000bc8653348b1fb7d25885908faa5fa355",
    "dotenv": "^16.4.7",
    "ethers": "6.13.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sonner": "^2.0.3",
    "three": "^0.175.0",
    "viem": "^2.25.0",
    "wagmi": "^2.14.16"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@tailwindcss/vite": "^4.1.3",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "@types/three": "^0.175.0",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.2.5",
    "wrangler": "^4.7.0"
  },
  "packageManager": "pnpm@9.5.0+sha512.140036830124618d624a2187b50d04289d5a087f326c9edfc0ccd733d76c4f52c3a313d4fc148794a2a9d81553016004e6742e8cf850670268a7387fc220c903"
}
